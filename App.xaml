<Application x:Class="FlashCard.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FlashCard"
             StartupUri="/View/SettingView.xaml">
    <Application.Resources>
        <FontFamily x:Key="EPKAISHO">pack://application:,,,/Asset/Font/EPKAISHO</FontFamily>
        <FontFamily x:Key="EPKYOUKA">pack://application:,,,/Asset/Font/#EPKYOUKA</FontFamily>
        <FontFamily x:Key="Handwriting">pack://application:,,,/Asset/Font/#Handwriting</FontFamily>
        <FontFamily x:Key="KanjiStrokeOrders">./Asset/Font/KanjiStrokeOrders.ttf#KanjiStrokeOrders</FontFamily>
    </Application.Resources>
</Application>
